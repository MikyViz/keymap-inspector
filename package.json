{
  "name": "keymap-inspector",
  "version": "0.1.3",
  "description": "Inspect keyboard events and characters across different layouts.",
  "main": "dist/index.js",
  "module": "dist/keymap-inspector.esm.js",
  "browser": "dist/keymap-inspector.umd.js",
  "types": "dist/index.d.ts",
  "bin": {
    "keymap-inspector": "dist/cli/index.js"
  },
  "scripts": {
    "build": "tsc",
    "build:browser": "rollup -c",
    "build:all": "npm run build && npm run build:browser",
    "start": "ts-node src/index.ts",
    "dev": "ts-node-dev --respawn src/index.ts",
    "cli": "npm run build && node dist/cli/index.js",
    "prepublishOnly": "npm run build:all",
    "test": "echo \"No tests yet\" && exit 0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MikyViz/keymap-inspector.git"
  },
  "keywords": [
    "keymap",
    "keyboard",
    "layout",
    "inspector",
    "i18n"
  ],
  "author": "vizenovsky@gmail.com",
  "license": "MIT",
  "files": [
    "dist/**/*",
    "README.md",
    "LICENSE"
  ],
  "devDependencies": {
    "@rollup/plugin-node-resolve": "^16.0.1",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^12.1.4",
    "@types/node": "^20.11.24",
    "rollup": "^2.79.2",
    "rollup-plugin-terser": "^7.0.2",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "tslib": "^2.8.1",
    "typescript": "^5.3.3"
  }
}
